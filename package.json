{
  "name": "app",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "start:sls": "sls offline start",
    "deploy": "sls deploy",
    "deploy-prod": "sls deploy --stage production --profile prod",
    "generate-types": "graphql-codegen",
    "test": "jest"
  },
  "dependencies": {
    "@graphql-modules/core": "^0.7.14",
    "@graphql-modules/di": "^0.7.14",
    "apollo-server": "^2.11.0",
    "apollo-server-lambda": "^2.14.2",
    "aws-sdk": "^2.668.0",
    "bcryptjs": "^2.4.3",
    "data-api-client": "^1.0.1",
    "graphql": "^14.6.0",
    "graphql-import-node": "0.0.4",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.7.6",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.10",
    "sql-query": "^0.1.27",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.13.5",
    "@graphql-codegen/typescript": "^1.13.5",
    "@graphql-codegen/typescript-resolvers": "^1.13.5",
    "@jagi/jest-transform-graphql": "^1.0.2",
    "@types/aws-lambda": "^8.10.17",
    "@types/jest": "^25.1.4",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/node": "^10.12.18",
    "@types/uuid": "^7.0.3",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "graphql-tag": "^2.10.3",
    "jest": "^25.1.0",
    "serverless": "^1.69.0",
    "serverless-offline": "^5.12.1",
    "serverless-webpack": "^5.2.0",
    "ts-jest": "^25.4.0",
    "ts-loader": "^5.3.3",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.8.3",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^1.7.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT",
  "jest": {
    "transform": {
      "\\.(gql|graphql)$": "@jagi/jest-transform-graphql",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
